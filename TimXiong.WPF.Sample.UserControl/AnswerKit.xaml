<UserControl x:Class="TimXiong.WPF.Sample.PutToWallControl.Answerkit"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TimXiong.WPF.Sample.PutToWallControl"
             mc:Ignorable="d" 
             d:DesignHeight="150" d:DesignWidth="100">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
        </Grid.RowDefinitions>

        <Button Grid.Row="0" Margin="5" Content="{Binding DebatchAddress}" IsEnabled="{Binding Enable}" />
        <Label Grid.Row="1" Height="40" Background="AliceBlue" Content="{Binding OrderNumber}" />
        <Label Grid.Row="2" Height="40" Background="White" Content="{Binding DisplayData}" />
        
    </Grid>
</UserControl>
